
import PresentationDsl;

String jsfPathFileName(String fullclassname) :
  fullclassname.replaceAll("\\.", "/") + ".xhtml" ;

String validKeyName(ModelElement element) :
	element.name.replaceAll(" ", ".");
	
String validIdentifier(ModelElement element) :
	element.name.replaceAll(" ", "_").replaceAll("\\.", "_");