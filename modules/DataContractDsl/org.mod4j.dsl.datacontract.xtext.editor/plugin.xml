<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.mod4j.dsl.datacontract.xtext.editor.DataContractDslEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="dtcmod"
            icon="icons/mod4j-datacontract.ico"
            id="org.mod4j.dsl.datacontract.xtext.editor.DataContractDslEditor"
            name="DataContractDsl Editor">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.newWizards">
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="org.mod4j.dsl.datacontract.xtext.wizards.NewDataContractDslProjectWizard"
			icon="icons/template.gif"
			id="org.mod4j.dsl.datacontract.xtext.wizards.NewDataContractDslProjectWizard"
			name="DataContractDsl Project"
			project="true">
		</wizard>
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="org.mod4j.dsl.datacontract.xtext.wizards.NewDataContractDslFileWizard"
			icon="icons/mod4j-datacontract.ico"
			id="org.mod4j.dsl.datacontract.xtext.xtext.wizards.NewDataContractDslFileWizard"
			name="DataContractDsl Editor file">
		</wizard>
	</extension>
</plugin>
