
extension org::mod4j::dsl::service::xtext::Extensions;
extension org::mod4j::dsl::service::xtext::Linking;

link(emf::EObject this) :
	this.allVisibleElements().internalLink();

private Void internalLink(emf::EObject this) :
	null;


private Void internalLink(ServiceDsl::CustomMethod this) :
	this.link_input()
 -> 	this.link_output()
;

private Void internalLink(ServiceDsl::SpecialMethod this) :
	this.link_dto()
;

private Void internalLink(ServiceDsl::CrudService this) :
	this.link_dto()
;


