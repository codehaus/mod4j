<?xml version="1.0" encoding="ASCII"?>
<xtext:XtextFile xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xtext="http://www.openarchitectureware.org/xtext" name="ServiceModel" preventMMGeneration="true">
  <mmImports location="http://www.mod4j.org/service"/>
  <rules xsi:type="xtext:TypeRule" name="ServiceModel">
    <type alias="" name="ServiceModel"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Assignment" cardinality="OPTIONAL" feature="description" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="STRING"/>
      </children>
      <children xsi:type="xtext:Keyword" value="Service"/>
      <children xsi:type="xtext:Assignment" feature="name" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="ID"/>
      </children>
      <children xsi:type="xtext:Assignment" cardinality="ANY" feature="dtoReferences" operator="ADD">
        <token xsi:type="xtext:RuleName" name="DtoReference"/>
      </children>
      <children xsi:type="xtext:Assignment" cardinality="ANY" feature="crudServices" operator="ADD">
        <token xsi:type="xtext:RuleName" name="CrudService"/>
      </children>
      <children xsi:type="xtext:Assignment" cardinality="ANY" feature="methods" operator="ADD">
        <token xsi:type="xtext:RuleName" name="ServiceMethod"/>
      </children>
      <children xsi:type="xtext:Keyword" value="end"/>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="DtoReference">
    <type alias="" name="DtoReference"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Keyword" value="import"/>
      <children xsi:type="xtext:Assignment" feature="name" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="ID"/>
      </children>
      <children xsi:type="xtext:Keyword" value=";"/>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="ServiceMethod">
    <type alias="" name="ServiceMethod"/>
    <content xsi:type="xtext:Alternatives">
      <alternatives xsi:type="xtext:RuleName" name="CustomMethod"/>
      <alternatives xsi:type="xtext:RuleName" name="SpecialMethod"/>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="CustomMethod">
    <type alias="" name="CustomMethod"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Assignment" cardinality="OPTIONAL" feature="description" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="STRING"/>
      </children>
      <children xsi:type="xtext:Keyword" value="method"/>
      <children xsi:type="xtext:Assignment" feature="name" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="ID"/>
      </children>
      <children xsi:type="xtext:Group" cardinality="OPTIONAL">
        <children xsi:type="xtext:Keyword" value="in"/>
        <children xsi:type="xtext:Assignment" feature="input" operator="ASSIGN">
          <token xsi:type="xtext:CrossReference">
            <type alias="" name="DtoReference"/>
          </token>
        </children>
      </children>
      <children xsi:type="xtext:Group" cardinality="OPTIONAL">
        <children xsi:type="xtext:Keyword" value="out"/>
        <children xsi:type="xtext:Assignment" feature="output" operator="ASSIGN">
          <token xsi:type="xtext:CrossReference">
            <type alias="" name="DtoReference"/>
          </token>
        </children>
      </children>
      <children xsi:type="xtext:Keyword" value=";"/>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="SpecialMethod">
    <type alias="" name="SpecialMethod"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Assignment" feature="type" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="MethodType"/>
      </children>
      <children xsi:type="xtext:Assignment" feature="name" operator="ASSIGN">
        <token xsi:type="xtext:RuleName" name="ID"/>
      </children>
      <children xsi:type="xtext:Keyword" value="for"/>
      <children xsi:type="xtext:Assignment" feature="dto" operator="ASSIGN">
        <token xsi:type="xtext:CrossReference">
          <type alias="" name="DtoReference"/>
        </token>
      </children>
      <children xsi:type="xtext:Keyword" value=";"/>
    </content>
  </rules>
  <rules xsi:type="xtext:TypeRule" name="CrudService">
    <type alias="" name="CrudService"/>
    <content xsi:type="xtext:Group">
      <children xsi:type="xtext:Keyword" value="crud"/>
      <children xsi:type="xtext:Keyword" value="for"/>
      <children xsi:type="xtext:Assignment" feature="dto" operator="ASSIGN">
        <token xsi:type="xtext:CrossReference">
          <type alias="" name="DtoReference"/>
        </token>
      </children>
      <children xsi:type="xtext:Keyword" value=";"/>
    </content>
  </rules>
  <rules xsi:type="xtext:EnumRule" name="MethodType">
    <type alias="" name="MethodType"/>
    <literals keyword="create" name="Create"/>
    <literals keyword="read" name="Read"/>
    <literals keyword="update" name="Update"/>
    <literals keyword="delete" name="Delete"/>
    <literals keyword="custom" name="Custom"/>
  </rules>
  <rules xsi:type="xtext:NativeLexerRule" name="ID"/>
  <rules xsi:type="xtext:NativeLexerRule" name="STRING"/>
  <rules xsi:type="xtext:NativeLexerRule" name="INT"/>
  <rules xsi:type="xtext:NativeLexerRule" name="WS"/>
  <rules xsi:type="xtext:NativeLexerRule" name="ML_COMMENT"/>
  <rules xsi:type="xtext:NativeLexerRule" name="SL_COMMENT"/>
</xtext:XtextFile>
