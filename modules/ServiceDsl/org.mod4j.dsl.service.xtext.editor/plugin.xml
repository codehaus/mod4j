<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.mod4j.dsl.service.xtext.editor.ServiceDslEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="sermod"
            icon="icons/mod4j-service.ico"
            id="org.mod4j.dsl.service.xtext.editor.ServiceDslEditor"
            name="Service Model Editor">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.newWizards">
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="org.mod4j.dsl.service.xtext.wizards.NewServiceDslProjectWizard"
			icon="icons/template.gif"
			id="org.mod4j.dsl.service.xtext.wizards.NewServiceDslProjectWizard"
			name="ServiceDsl Project"
			project="true">
		</wizard>
         
		<wizard
            category="org.mod4j.dsl.model" 
			class="org.mod4j.dsl.service.xtext.wizards.NewServiceDslFileWizard"
			icon="icons/mod4j-service.ico"
			id="org.mod4j.dsl.service.xtext.wizards.NewServiceDslFileWizard"
			name="ServiceDsl Editor file">
		</wizard>
	</extension>
</plugin>
