import BusinessDomainDsl;

context BusinessClass ERROR
	"Names of BusinessClass attributes must be unique.":
	properties.forAll(a1| properties.notExists(a2| a1 != a2 && a1.name == a2.name ) );

context Property ERROR
	"The dataType of a property must be specified.":
	dataType.length > 1;

context StringProperty ERROR 
	"StringProperties must be more than one char long." :
	name.length > 1;




